{
	"name": "fortytwo-studio/entry-templates",
	"description": "Create pre-filled templates which can be selected within entries.",
	"type": "craft-plugin",
	"version": "1.1.0",
	"license": "MIT",
	"authors": [
		{
			"name": "Forty Two studio",
			"homepage": "https://fortytwo.studio"
		}
	],
	"require": {
		"craftcms/cms": "^5.0.0"
	},
	"autoload": {
		"psr-4": {
			"fortytwostudio\\entrytemplates\\": "src/"
		}
	},
	"extra": {
		"name": "Entry Templates",
		"handle": "entrytemplates",
		"developer": "Forty Two Studio",
		"developerUrl": "https://fortytwo.studio",
		"documentationUrl": "https://github.com/fortytwostudio/entry-templates/blob/main/README.md",
		"changelogUrl": "https://raw.githubusercontent.com/fortytwostudio/entry-templates/main/CHANGELOG.md",
		"class": "fortytwostudio\\entrytemplates\\EntryTemplates"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
