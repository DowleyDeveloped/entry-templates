{
	"name": "dowleydeveloped/entry-templates",
	"description": "Create pre-filled templates which can be selected within entries.",
	"type": "craft-plugin",
	"version": "1.0.0",
	"license": "MIT",
	"authors": [
		{
			"name": "Dowley Developed",
			"homepage": "https://charlottedowley.co.uk"
		}
	],
	"require": {
		"craftcms/cms": "^5.0.0"
	},
	"autoload": {
		"psr-4": {
			"dowleydeveloped\\entrytemplates\\": "src/"
		}
	},
	"extra": {
		"name": "Entry Templates",
		"handle": "entrytemplates",
		"developer": "Dowley Developed",
		"developerUrl": "https://charlottedowley.co.uk",
		"documentationUrl": "https://github.com/dowleydeveloped/entry-templates/blob/main/README.md",
		"changelogUrl": "https://raw.githubusercontent.com/dowleydeveloped/entry-templates/main/CHANGELOG.md",
		"class": "dowleydeveloped\\entrytemplates\\EntryTemplates"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
